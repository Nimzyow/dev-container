// See https://containers.dev/implementors/json_reference/ for configuration reference
{
  "name": "Untitled Node.js project",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "remoteUser": "node",
  "postCreateCommand": "bash -c \"$(curl -fsSL https://raw.githubusercontent.com/ohmybash/oh-my-bash/master/tools/install.sh)\"",
  "features": {
    "ghcr.io/devcontainers/features/aws-cli:1": {},
    "ghcr.io/devcontainers/features/docker-in-docker:2": {},
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/python:1": {},
    "ghcr.io/devcontainers-contrib/features/ansible:2": {},
    "ghcr.io/devcontainers-contrib/features/curl-apt-get": {},
    "ghcr.io/devcontainers-contrib/features/wget-apt-get": {},
    "ghcr.io/devcontainers-contrib/features/groovy-sdkman:2": {},
    "ghcr.io/devcontainers-contrib/features/maven-sdkman:2": {},
    "ghcr.io/devcontainers-contrib/features/terraform-asdf:2": {},
    "ghcr.io/devcontainers-contrib/features/typescript:2": {},
    "ghcr.io/customink/codespaces-features/sam-cli:1": {},
    "ghcr.io/guiyomh/features/vim:0": {}
  },
  "customizations": {
    "vscode": {
      "extensions": ["GitHub.copilot", "vscjava.vscode-java-pack"]
    }
  }
}
